@model DashboardViewModel
<h3 class="page-title">
    Member : @Model.NumberOfTotalMember
</h3>
<div class="row">
    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="dashboard-stat blue">
            <div class="visual">
                <i class="fa fa-comments"></i>
            </div>
            <div class="details">
                <div class="number">
                    <span data-counter="counterup" data-value="1349">Advisor : @Model.NumberOfAdvisor</span>
                </div>
                <div class="desc"> </div>
            </div>
            <a class="more" asp-controller="Member" asp-action="Manage" asp-route-id="1">
                View Advisors
                <i class="m-icon-swapright m-icon-white"></i>
            </a>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="dashboard-stat green-meadow">
            <div class="visual">
                <i class="fa fa-globe"></i>
            </div>
            <div class="details">
                <div class="number">
                    <span data-counter="counterup" data-value="89"></span> Executive : @Model.NumberOfExecutive
                </div>
                <div class="desc"> </div>
            </div>
            <a class="more" asp-controller="Member" asp-action="Manage" asp-route-id="2">
                View Executive
                <i class="m-icon-swapright m-icon-white"></i>
            </a>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="dashboard-stat purple">
            <div class="visual">
                <i class="fa fa-globe"></i>
            </div>
            <div class="details">
                <div class="number">
                    <span data-counter="counterup" data-value="89"></span> Members : @Model.NumberOfMember
                </div>
                <div class="desc"></div>
            </div>
            <a class="more" asp-controller="Member" asp-action="Manage" asp-route-id="3">
                View Members
                <i class="m-icon-swapright m-icon-white"></i>
            </a>
        </div>
    </div>

</div>

<div class="clearfix"></div>

<h3 class="page-title">
    Quick Menu
</h3>
<div class="row">
    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="dashboard-stat yellow-gold">
            <div class="visual">
                <i class="fa fa-comments"></i>
            </div>
            <div class="details">
                <div class="number">
                    <span data-counter="counterup" data-value="1349">Send SMS</span>
                </div>
                <div class="desc"> </div>
            </div>
            <a class="more" asp-controller="Sms" asp-action="Manage">
                View SMS
                <i class="m-icon-swapright m-icon-white"></i>
            </a>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="dashboard-stat green">
            <div class="visual">
                <i class="fa fa-comments"></i>
            </div>
            <div class="details">
                <div class="number">
                    <span data-counter="counterup" data-value="1349">Report</span>
                </div>
                <div class="desc"> </div>
            </div>
            <a class="more" asp-controller="Report" asp-action="Manage">
                View Report
                <i class="m-icon-swapright m-icon-white"></i>
            </a>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="dashboard-stat blue">
            <div class="visual">
                <i class="fa fa-comments"></i>
            </div>
            <div class="details">
                <div class="number">
                    <span data-counter="counterup" data-value="1349">Form Collection</span>
                </div>
                <div class="desc"> </div>
            </div>
            <a class="more" asp-controller="AdmissionForm" asp-action="Manage">
                View Form
                <i class="m-icon-swapright m-icon-white"></i>
            </a>
        </div>
    </div>
</div>

<div class="clearfix"></div>

<h3 class="page-title">
    Help
</h3>
<div class="row">
    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="dashboard-stat blue">
            <div class="visual">
                <i class="fa fa-comments"></i>
            </div>
            <div class="details">
                <div class="number">
                    <span data-counter="counterup" data-value="1349">Help : @Model.NumberOfHelp</span>
                </div>
                <div class="desc"> </div>
            </div>
            <a class="more" asp-controller="HelpCollection" asp-action="Manage">
                View Help
                <i class="m-icon-swapright m-icon-white"></i>
            </a>
        </div>
    </div>
</div>

